@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <form method="post">
        @Html.DisplayNameFor(model => model.StringInput)
        <input asp-for="StringInput"/>     
        <input type="submit">      
    </form>
    
    <div>
    Result
        <p>Input: @Model.StringResult.Input</p>
        <p>Length: @Model.StringResult.Length</p>
        <p>CacheHit: @Model.StringResult.CacheHit</p>
    </div>
</div>


